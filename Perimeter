package MyMath
public class Perimeter {
	int TrianglePerimeter(int length1, int length2, int length3) {
		int result = length1+length2+length3;
		System.out.println("Perimeter of the triangle is "+result)
		return result;
	}
	int RectanglePerimeter(int length1, int length2, int length3, length4) {
		int result = length1+length2+length3+length4;
		System.out.println("Perimeter of the rectangle is "+result)
		return result;
		
	}
	int SquarePerimeter(int length1) {
		int result = length1*4;
		System.out.println("Perimeter of the square is "+result)
		return result;

}
