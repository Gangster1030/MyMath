package MyMath;

public class Geometric {

	public int Multiply(int x, int y) {
		int z = x * y;
		System.out.println("Multiplication of " + x + " and " + y + " = " + z);
		
		return z;
	}
	public int Division(int x, int y) {
		int z=x/y
		
		System.out.println("Division of "+x+" and "+y+" = "+z)
	}	return z;
